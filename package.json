  "scripts": {
    "start": "node app.js",
    "debug": "node inspect app.js",
    "frontend:start": "DUMMY_PULLS=./frontend/dummy-pulls.json webpack-dev-server --config ./frontend/webpack.dev.config.js",
    "frontend:watch": "webpack --watch --config ./frontend/webpack.dev.config.js",
    "frontend:build": "webpack --config ./frontend/webpack.prod.config.js",
